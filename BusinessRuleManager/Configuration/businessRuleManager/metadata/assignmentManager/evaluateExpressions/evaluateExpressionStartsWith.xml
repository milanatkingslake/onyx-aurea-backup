<rules xmlns="http://www.consonacrm.com/brm"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.consonacrm.com/brm ../../../grammar/onyx/businessRuleManager.grammar.onyx.xsd"
	   description="Implement startsWith Operator">
	<setNodeValue>
		<path>
			<literal>./expressionIsTrue</literal>
		</path>
		<value>
			<stringEqualTo>
				<left>
					<substring>
						<start>
							<literal>0</literal>
						</start>
						<length>
							<stringLength>
								<getNodeValue>
									<literal>c3</literal>
								</getNodeValue>
							</stringLength>
						</length>
						<value>
							<getNodeValue>
								<literal>valueToCompare</literal>
							</getNodeValue>
						</value>
					</substring>
				</left>
				<right>
					<getNodeValue>
						<literal>c3</literal>
					</getNodeValue>
				</right>
			</stringEqualTo>
		</value>
	</setNodeValue>
</rules>