<expressions>
	<expression name="equals" value="=" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionIsEqual.xml" />
	<expression name="isNotEqual" value="!=" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionIsNotEqual.xml" />
	<expression name="hasAValue" value="hasAValue" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionHasAValue.xml" />
	<expression name="greaterThan" value="&gt;" allowNulls="false" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionGreaterThan.xml" />
	<expression name="lessThan" value="&lt;" allowNulls="false" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionLessThan.xml" />
	<expression name="startsWith" value="startsWith" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionStartsWith.xml" />
	<expression name="isInList" value="isInList" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionIsInList.xml" />
	<expression name="between" value="between" allowNulls="false" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionBetween.xml" />
  <expression name="handleNull" value="handleNull" allowNulls="true" ruleSetPath="assignmentManager\evaluateExpressions\evaluateExpressionsHandleNull.xml" />
</expressions>