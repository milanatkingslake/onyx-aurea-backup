<businessRuleManager>
	<!--List of all assemblies that contain grammar classes.-->
	<modules>
		<module name="Onyx" assembly="ConsonaCRM.BusinessRuleManager.Onyx" />
	</modules>
	<!--List of all grammar keywords and the class that handles them.-->
	<grammar>
		<!--Onyx-specific operations-->
		<keyword name="returnMessage" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.ReturnMessage"/>
		<keyword name="objectInstance" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.ObjectInstance"/>
		<keyword name="otmCall" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.OTMCall"/>
		<keyword name="hasPermission" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.HasPermission"/>
		<keyword name="setSessionItem" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.SetSessionItem"/>
		<keyword name="getSessionItem" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.GetSessionItem"/>
		<keyword name="removeSessionItem" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.RemoveSessionItem"/>
		<keyword name="logValue" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.LogValue"/>
		<keyword name="logNode" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.LogNode"/>
		<keyword name="stepAdditionalData" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.StepAdditionalData"/>
		<keyword name="odbcConvertToRowSet" module="Onyx" class="ConsonaCRM.BusinessRuleManager.Grammar.Expressions.Onyx.OdbcConvertToRowSet"/>
	</grammar>
</businessRuleManager>
